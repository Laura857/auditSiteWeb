{
  "routes": [
    {
      "src": ".*",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY"
      },
      "continue": true
    },
    {
      "src": "(.*).assests.(css|js)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
      },
      "continue": true
    }
  ]
}